
<h1><%= @user.first_name %> <%= @user.last_name %></h1>
<h2>Welcome to your Dashboard!</h2>

<h3>My proposals</h3>
<ul>
    <% @proposals.each do |proposal| %>
        <li>
            <p><%= proposal.title %></p>
            <p><%= proposal.description %></p>
            <p><%= link_to "Proposal Page", proposal_path(proposal)  %>
        </li>
    <% end  %>
</ul>


